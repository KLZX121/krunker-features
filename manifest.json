{
  "name": "Krunker Features",
  "description" : "Krunker extension that provides helpful features - these are NOT hacks",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://krunker.io/*"],
      "js": ["./features/menuTimer.js", "./features/findNewGame.js", "main.js"]
    }
  ],
  "icons": {
    "16": "imgs/icon_16.png",
    "48": "imgs/icon_48.png",
    "128": "imgs/icon_128.png" 
  }
}